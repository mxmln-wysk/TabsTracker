{
    "name": "TabsTracker",
    "description": "Simple Extension to track activity",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["tabs","activeTab", "storage"],
    "action":{
        "default_popup": "popup.html"
    }
}
